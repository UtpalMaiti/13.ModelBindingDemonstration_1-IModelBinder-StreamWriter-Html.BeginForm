@model List<ModelBindingDemonstration_1.Models.Employee>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NewView</title>
</head>
<body>
    <div>
        <h2>Welcome to New View to Display the Data...</h2>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Location</th>
                    <th>Salary</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var emp in Model)
                {
                    <tr>
                        <td>@emp.Id</td>
                        <td>@emp.Name</td>
                        <td>@emp.Location</td>
                        <td>@emp.Salary</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
